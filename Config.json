{
        "QuickStep_Shell_Dir": "/fastdisk/quickstep-datalog/build",
        "Input_Dir": "./Input",
        "Logging": {
                "logging_directory": "./log",
                "log": true,
                "logging_level": {
                        "stdout": true,
                        "info_log": true
                }
        },
        "QuickStep": {
                "csv_delimiter": ","
        },
        "CQA": {
                "rewriting": true,
                "algorithm": "pair-pruning",
                "pair_pruning_rules": true
        },        
        "Execution": {
                "backend": "sql-server",
                "single_query_evaluation": true
        },
        "Input": {
                "preload": true
        },
	"Output": {
		"write_to_csv": false,
                "retain_final_output_only": true,
                "final_output_relations": ["return"] 
	}, 
        "Debug": {
                "static_debug": true,
                "dynamic_debug": false,
                "dynamic_debug_iter_num": 1,
                "cost_model_check": false,
                "interpret": true
        },
        "Optimization": {
                "analyzer_level": "minimum",
		"analyze_all_tables": false,
                "dynamic_set_diff": true,
                "default_set_diff_alg": 2,
		"selective_dedup": true,
		"dedup_relation_list": ["return"],
                "unified_idb_evaluation": false
        },
        "Parameters": {
                "threads_num": 40,
                "block_size": 262144
        }
}
